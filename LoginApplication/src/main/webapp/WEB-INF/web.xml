<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<servlet>
		<servlet-name>MyServlet</servlet-name>
		<display-name>MyServlet</display-name>
		<description></description>
		<servlet-class>com.bridge.controller.MyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DisplayServlet</servlet-name>
		<display-name>DisplayServlet</display-name>
		<description></description>
		<servlet-class>com.bridge.controller.DisplayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyServlet</servlet-name>
		<url-pattern>/servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DisplayServlet</servlet-name>
		<url-pattern>/DisplayServlet</url-pattern>
	</servlet-mapping>
</web-app>