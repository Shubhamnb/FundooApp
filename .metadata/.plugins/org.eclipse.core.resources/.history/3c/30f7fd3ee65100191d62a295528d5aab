package com.bridge.Hash;

public class Hashing {
	Object[] arr ;
	public Hashing() {
		arr = new Object[10];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = new int[5];
		}
	}
	public void addSloat(int num,int pos) {
		for (int i = 0; i < arr.length; i++) {
			Object ar = arr[pos]; 
			
		}
	}
	public void add(int n) {
		int num = 0 ;
		num = n % 11;
		for (int i = 0; i < arr.length; i++) {
			if(num == i) {
				addSloat(n, i);
			}
		}
	}
}
