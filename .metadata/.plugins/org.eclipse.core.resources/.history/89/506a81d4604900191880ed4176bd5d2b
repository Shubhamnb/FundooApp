package shubham.java;

import java.util.Scanner;

public class First1000PrimeNumber {

	public static void main(String[] args) {
		
		
			int count=0;
			int i=2;
			int arr[] = new int[1000];
			
			System.out.println("Shubham");
			
			while(count!=1000) {
				if(i==2) {
					count++;
					System.out.print (i+" ");
					i++;
					continue;
					
					
				}
				for(int j=2;j<i;j++) {
					if(i%j == 0) {
						break;
					}
					if(j == (i-1)) {
						System.out.print(i+" ");
						count++;
					}
				}
				
				i++;
			}
	}

}
