package com.bridge.Hash;



import com.bridge.OrderedList.LinkedList1;

public class Hashing {
	LinkedList1[] list = new LinkedList1[10];

	public void addHash(int num) {
		int mod = num % 11;
		list[mod] = new LinkedList1();
		list[mod].add(num);
	}

	public void showHash() {
		for (int i = 0; i < list.length; i++) {
			if(list[i] == null) {
				System.out.println("[///////]");
			}else {
				System.out.print("[");
				list[i].show();
				System.out.print("]");
			}
		}
	}
	
}
