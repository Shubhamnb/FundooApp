package com.bridge.LinkedList;

public class Node1 {
	int data;
	Node1 next;
	
	
	public Node1(int data, Node1 next) {
		this.data = data;
		this.next = next;
	}
	
	
	public int getData() {
		return data;
	}
	public void setData(int data) {
		this.data = data;
	}
	public Node getNext() {
		return next;
	}
	public void setNext(Node next) {
		this.next = next;
	}
	
	
}
