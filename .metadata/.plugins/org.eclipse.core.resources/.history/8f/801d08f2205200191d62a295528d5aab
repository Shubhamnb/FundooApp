package com.bridge.BinarySearchTree;

public class BinarySearchTree {
	Node root;
	Node node;
	public Node add(int num, Node nod) {
		 
		if(root == null) {
			node = new Node(num, null, null);
			root = node;
		}else {
			Node temp = root;
			if(temp.data < num) {
				root = temp.next;
				return num, root;
			}else {
				root = temp.prev;
				return num,root;
			}
		}
	}

	
	
	
}
