package com.bridge.prime;

public class PrimeAnagram {
	
	public static boolean isAnagram(String str1, String str2) {
		
		return true;
	}
	
	public static void isAnagrame(int[][] ar, int[] cn) {
		
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < cn[i]-1; j++) {
				for (int k = 1; k < cn.length; k++) {
					String str1 = Integer.toString(ar[i][j]);
					String str2 = Integer.toString(ar[i][k]);
					if(isAnagram(str1, str2))
						System.out.print(" "+ar[i][j]);
				}
			}
			System.out.println();
		}
	}

}
