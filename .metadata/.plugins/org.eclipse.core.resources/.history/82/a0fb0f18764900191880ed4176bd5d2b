package shubham.java;

import java.util.Scanner;

public class First1000PrimeNumber {

	
	
	public static void anagramPrimeNumber(int ar[]) {
		int key1 = 0, key2 = 0;
		int m1 = 0, m2 = 0;
		for (int i = 0; i < (ar.length-1); i++) {
			for (int j = i+1; j < ar.length; j++) {
				key1 = ar[i];
				key2 = ar[j];
				/*
				 * if(key1 == 13) { System.out.println(ar[i]); System.out.println(ar[j]); }
				 */
				while(key1 != 0 && key2 != 0 ) {
					m1 = m1 + key1 % 10;
					key1 = key1 / 10;
					m2 = m2 + key2 % 10;
					key2 = key2 / 10;
				}
				if(m1 == m2) {
					System.out.println(ar[i]+" "+ar[j]);
				}
			}
		}
	}
	public static void main(String[] args) {
			int count=0,c=0;
			int i=2;
			int arr[] = new int[168];
			int k=0;
			
			//System.out.println("Shubham");
			
			while(i < 1000) {
				if(i==2) {
					count++;
					System.out.print (i+" ");
					arr[k] = i;
					k++;
					i++;
					continue;
				}
				for(int j=2;j<i;j++) {
					if(i%j == 0) {
						break;
					}
					if(j == (i-1)) {
						arr[k] = i;
						System.out.print(i+" ");
						count++;
						k++;
					}
				}
				
				i++;
			}
			System.out.println();
		
		  for (int j : arr) { System.out.print(j+" "); }
		  
		  System.out.println();
			anagramPrimeNumber(arr);
		 
	}

}
